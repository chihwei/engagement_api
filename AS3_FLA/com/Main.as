package{	import flash.display.Sprite;	import com.socialvibe.engagement.api.SocialVibeProxy;	import flash.events.Event;	import flash.events.MouseEvent;	import com.ui.SvButton;		public class Main extends Sprite	{		private var _api:SocialVibeProxy;				public function Main()		{			// 1. Listen for the ADDED_TO_STAGE event.  You will not be able to initialize the API or access the 'stage' until then.			this.addEventListener(Event.ADDED_TO_STAGE, init);		}				private function init(e:Event):void		{			this.removeEventListener(Event.ADDED_TO_STAGE, init);						// 2. Initialize the API			_api = new SocialVibeProxy();						// This is a very simple engagement.  Clicking on this square, makes the 'engage()' call to credit the user their user benefit (i.e. Farm Cash)			//This particular engagement ends when the user clicks this button.			var button:SvButton = new SvButton();			this.addChild(button);					button.x = this.stage.stageWidth/2 - button.width/2;			button.y = this.stage.stageHeight/2 - button.height/2;					button.useHandCursor = button.buttonMode = true;			button.addEventListener(MouseEvent.CLICK, onEngage);		}				private function onEngage(e:Event):void		{			// 3. Call to engage()			_api.engage();						// 4. Call endEngage() when you want the engagement to end and to show the user a 'Congrats & Share' screen.  Note: This can happen anytime after the call to engage()			_api.endEngage();		}	}}